{
  "submenu#department-content": {
    "props": {
      "blockClass": ["departmentSubmenu", "departmentSubmenuExtra"]
    },
    "children": ["flex-layout.row#department-content"],
    "title": "Categories"
  },
  "flex-layout.row#department-content": {
    "children": ["flex-layout.row#department-content-01", "flex-layout.row#department-content-02"],
    "props": {
      "fullWidth": true,
      "preventHorizontalStretch": true,
      "blockClass": "department-content"
    }
  },
  "flex-layout.row#department-content-01": {
    "children": ["flex-layout.col#department-content-01-col-01", "flex-layout.col#department-content-01-col-02"],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "department-content-01"
    }
  },
  "flex-layout.row#department-content-02": {
    "children": ["flex-layout.col#department-content-02-col-01", "flex-layout.col#department-content-02-col-02"],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "department-content-02"
    }
  },
  "flex-layout.col#department-content-01-col-01": {
    "children": ["flex-layout.row#department-content-column-01"],
    "props": {
      "blockClass": "department-content-col"
    }
  },
  "flex-layout.col#department-content-01-col-02": {
    "children": ["flex-layout.row#department-content-column-02"],
    "props": {
      "blockClass": "department-content-col"
    }
  },
  "flex-layout.col#department-content-02-col-01": {
    "children": [
      "flex-layout.col#department-content-02-col-01-image-01",
      "flex-layout.col#department-content-02-col-01-image-02"
    ],
    "props": {
      "blockClass": "department-content-col"
    }
  },
  "flex-layout.col#department-content-02-col-01-image-01": {
    "children": ["rich-text#submenu-image-title", "image#submenu-image-example-01"],
    "props": {
      "preventVerticalStretch": true
    }
  },
  "rich-text#submenu-image-title": {
    "props": {
      "text": "[Image title](/)",
      "blockClass": "submenu-image-title"
    }
  },
  "image#submenu-image-example-01": {
    "props": {
      // "src": "assets/menu/example1.png",
      "link": {
        "url": "/"
      },
      "blockClass": "submenu-image"
    }
  },
  "image#submenu-image-example-02": {
    "props": {
      // "src": "assets/menu/example2.png",
      "link": {
        "url": "/"
      },
      "blockClass": "submenu-image"
    }
  },
  "flex-layout.col#department-content-02-col-01-image-02": {
    "children": ["rich-text#submenu-image-title", "image#submenu-image-example-02"],
    "props": {
      "preventVerticalStretch": true
    }
  },
  "flex-layout.col#department-content-02-col-02": {
    "children": ["rich-text#submenu-image-title", "image#submenu-image-example-03"],
    "props": {
      "blockClass": "department-content-col"
    }
  },
  "image#submenu-image-example-03": {
    "props": {
      // "src": "assets/menu/example3.png",
      "link": {
        "url": "/"
      },
      "blockClass": "submenu-image"
    }
  },
  "flex-layout.row#department-content-column-01": {
    "children": ["menu#department-content-column-template"],
    "props": {
      "blockClass": ""
    },
    "title": "Menu column 1"
  },
  "flex-layout.row#department-content-column-02": {
    "children": ["menu#department-content-column-template"],
    "props": {
      "blockClass": ""
    },
    "title": "Menu column 2"
  },

  "menu#department-content-column-template": {
    "children": ["disposition-layout#category-template"],
    "props": {
      "orientation": "vertical",
      "blockClass": ["all-departments-menu", "all-departments-menu-extra"]
    }
  },
  "disposition-layout#category-template": {
    "children": [
      "menu#unleashed-menu-desktop-category-01",
      "menu#unleashed-menu-desktop-category-02",
      "menu#unleashed-menu-desktop-category-03",
      "menu#unleashed-menu-desktop-category-04",
      "menu#unleashed-menu-desktop-category-05",
      "menu#unleashed-menu-desktop-category-06",
      "menu#unleashed-menu-desktop-category-07",
      "menu#unleashed-menu-desktop-category-08"
    ],
    "props": {
      "disposition": [
        { "order": 1, "show": true },
        { "order": 2, "show": true },
        { "order": 3, "show": true },
        { "order": 4, "show": true },
        { "order": 5, "show": true },
        { "order": 6, "show": true },
        { "order": 7, "show": true },
        { "order": 8, "show": true }
      ]
    },
    "title": "Disposition - Menu column category"
  },
  "menu#unleashed-menu-desktop-category-01": {
    "children": ["menu-item#unleashed-menu-desktop-category-template", "disposition-layout#subcategory-template"],
    "props": {
      "blockClass": "menu-desktop",
      "orientation": "vertical"
    },
    "title": "Category 01"
  },
  "menu#unleashed-menu-desktop-category-02": {
