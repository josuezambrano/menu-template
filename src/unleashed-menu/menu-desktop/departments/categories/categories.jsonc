  "menu-item#unleashed-menu-desktop-category-template": {
    "props": {
      "blockClass": ["submenu-title", "submenu-title-extra"],
      "id": "unleashed-menu-desktop-category-template",
      "type": "custom",
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/",
        "noFollow": false,
        "tagTitle": "#category",
        "text": "#category"
      }
    }
  }
